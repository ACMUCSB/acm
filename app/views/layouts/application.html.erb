<!DOCTYPE html>
<html>

  <head>
    <title>ACM@UCSB</title>
    <%= stylesheet_link_tag 'application' %>
    <%# javascript_include_tag :defaults %>
    <%= csrf_meta_tag %>
  </head>

  <body>

    <div id="container">

      <div id="header">
        <h1 id="title"><%= link_to 'ACM@UCSB', root_path %></h1>
        <ul id="nav">
          <li><%= link_to 'Announcements', root_path %></li>
          <li><%= link_to 'Forum', '' %></li>
          <li><%= link_to 'About', about_path %></li>
          <li><%= link_to 'Contact', contact_path %></li>
          <li><%= link_to 'Sitemap', '/sitemap.txt' %></li>
        </ul>
      </div>

      <ol id="timeline">
      	<div class="event">
      		<a href=""></a>
      		<div class="eventText">
      			<h4>General Meeting #1</h4>
	      		<p class="eventTime">p.eventTime</p>      		
	      		<p>Description test test test test test</p>
      		</div>      		
      	</div>
      	
      	<div class="event">
      		<a href=""></a>
      		<div class="eventText">
      			<h4>General Meeting #2</h4>
	      		<p class="eventTime">p.eventTime</p>      		
	      		<p>Description test test test test test</p>
      		</div>      		
      	</div>
      	
      	
        <!--<li>GENERAL MEETING #1</li>
        <li>GENERAL MEETING #2</li>
        <li>GENERAL MEETING #3</li>-->
      </ol>

      <div id="content">

        <%= yield %>

      </div>

      <div id="footer"></div>

    </div>

  </body>
</html>
